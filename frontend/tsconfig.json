{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"~/*": ["./src/*"],
			"$lib/*": ["./src/lib/*"]
		},
		"types": ["@types/node", "./worker-configuration.d.ts"],
		"noUnusedLocals": true,
		"noUnusedParameters": true
	},
	"references": [{ "path": "../backend" }]
}
