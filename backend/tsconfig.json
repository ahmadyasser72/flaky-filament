{
	"compilerOptions": {
		"composite": true,
		"baseUrl": ".",
		"paths": {
			"~/*": ["./src/*"]
		},
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"lib": ["ESNext"],
		"types": ["@types/node", "./worker-configuration.d.ts"],
		"noUnusedLocals": true,
		"noUnusedParameters": true
	}
}
